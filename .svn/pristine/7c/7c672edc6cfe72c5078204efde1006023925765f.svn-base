*{margin: 0; padding: 0;}
.nav{
	font-size: 13px;
	width: 100%;
	height: 34px;
	color: #606060;
	background-color: #f5f5f5;
	margin-bottom: 0;
}
.nav-content{
	top: 0;
	width: 875px;
	height: 34px;
	margin: auto;
	margin: 0 auto;
	position: relative;
}
.nav-wel{
	float: left;
	height: 34px;
	min-width: 240px;
	line-height: 34px;
}
.nav-wel span{margin-right: 22px;}
.nav-wel span a{color: #606060; text-decoration: none;}
.nav-ul{min-width: 250px; float: right;}
.nav-ul ul{list-style: none; margin: 10px 0;}
.nav-ul li{float: left; margin: 0 5px;}
.nav-ul li a{text-decoration: none; color: #606060;}
.nav-ul li a:hover{color: #fff;};


.logo{width: 100%; height: 90px;}
.logo-content{width: 764px; height: 90px; margin: 0 auto;}
.logo-logo{margin-top: 16px; width: 313px; float: left;}
.logo-shop{margin-top: 11px; width: 160px; height: 70px; float: left;}
.logo-btngroup{	width: 290px; height: 40px; float: right; margin-top: 35px;}
.btn-input{float:right;height: 32px; width: 218px; border: 2px solid #7fe1f9;}
.logo-btn{
	float:right;
	color: white;
	width: 67px;
	height: 32px;
	background-color: #7fe1f9;
	display: inline-block;
	line-height: 31px;
	text-align: center;
	margin: auto;
	text-decoration: none;
}
@-moz-document url-prefix() {
	.logo-btn { 
		line-height: 36px;
		height: 36px;
	}
	.btn-input{
		height: 32px;
	}
}


.guide{width: 100%; height: 33px; margin-bottom: 6px; background-color: #333;}
